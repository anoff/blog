@startuml arrows
skinparam monochrome true
skinparam cardBackgroundColor white
card "<&arrow-right>" as 1
card "--|>" as 2
1 --|> 2
card "--*" as 3
1 --* 3
card "--o" as 4
1 --o 4
card "-->" as 8
1 --> 8
card "-->>" as 9
1 -->> 9
card "*-->" as 11
1 *--> 11
card "--#" as 12
1 --# 12
@enduml